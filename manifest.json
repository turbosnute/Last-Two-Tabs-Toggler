{
    "update_url": "https://clients2.google.com/service/update2/crx",
    "name": "Last 2 Toggle",
    "version": "1.0",
    "description": "Toggle between the two latest tabs using Ctrl+Space",
    "manifest_version": 3,
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "action": {},
    "commands": {
        "toggle_tabs": {
            "suggested_key": {
                "default": "Ctrl+Space",
                "mac": "MacCtrl+Space"
            },
            "description": "Toggle between the two latest tabs"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs"
    ]
}